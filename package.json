{
  "name": "buckle",
  "version": "0.1.0",
  "scripts": {
    "postinstall": "esy",
    "clean": "esy bsb -clean",
    "clean:all": "esy bsb -clean-world",
    "build:bsb": "esy bsb -make-world -theme basic-reason",
    "build:js": "webpack",
    "build": "run-s build:bsb build:js",
    "watch:bsb": "esy bsb -make-world -w -theme basic-reason",
    "watch:js": "webpack-dev-server",
    "watch": "run-p watch:bsb watch:js",
    "start": "run-s watch"
  },
  "keywords": [ "BuckleScript" ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "esy":"0.6.10",
    "bucklescript-tea": "^0.15.0",
    "npm-run-all": "^4.1.3",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "bs-css-emotion": "^3.0.0"
  }
}
